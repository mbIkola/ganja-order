type Order {
    id: ID! @id
    status: String!
    paid: Boolean!
    price: String!
    size: String!
    type: String!
    name: String!
    phone: String!
    country: String,
    state: String,
    city: String,
    zipCode: String,
    address_line1: String,
    address_line2: String,
    wallet: String,
    walletName: String,
    rate: String,
    copayerId: String
}

type Future {
    id: ID! @id
    status: String!
    paid: Boolean
    price: String
    size: String
    type: String
    name: String
    phone: String
    country: String,
    state: String,
    city: String,
    zipCode: String,
    address_line1: String,
    address_line2: String,
    wallet: String,
    walletName: String,
    rate: String,
    copayerId: String
}
